<Application
    x:Class="EuphoricElephant.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:EuphoricElephant"
    xmlns:converter="using:EuphoricElephant.Converters"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <local:ViewModelLocator x:Key="viewModelLocator"></local:ViewModelLocator>
            <converter:SecondsToTimeConverter x:Key="secondsToTimeConverter"></converter:SecondsToTimeConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>
